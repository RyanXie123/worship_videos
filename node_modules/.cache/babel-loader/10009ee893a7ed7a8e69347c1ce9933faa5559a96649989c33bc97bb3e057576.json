{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'VideoGrid',\n  data() {\n    return {\n      videos: []\n    };\n  },\n  mounted() {\n    axios.get('https://router.xieru.fun:12315/api/fs/list').then(response => {\n      this.videos = response.data.data.content;\n    }).catch(error => {\n      console.error(error);\n    });\n  },\n  methods: {\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toLocaleDateString();\n    },\n    formatSize(size) {\n      const fileSizeMB = (size / (1024 * 1024)).toFixed(2);\n      return `${fileSizeMB} MB`;\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","videos","mounted","get","then","response","content","catch","error","console","methods","formatDate","dateString","date","Date","toLocaleDateString","formatSize","size","fileSizeMB","toFixed"],"sources":["/home/worship_video/src/components/VideoGrid.vue"],"sourcesContent":["<template>\n    <div class=\"video-grid\">\n        <div class=\"video-item\" v-for=\"video in videos\" :key=\"video.name\">\n            <img :src=\"video.thumb\" alt=\"Video Thumbnail\" class=\"video-thumbnail\" />\n            <div class=\"video-info\">\n                <h3 class=\"video-name\">{{ video.name }}</h3>\n                <p class=\"video-date\">Created: {{ formatDate(video.created) }}</p>\n                <p class=\"video-size\">Size: {{ formatSize(video.size) }}</p>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n    name: 'VideoGrid',\n    data() {\n        return {\n            videos: [],\n        };\n    },\n    mounted() {\n        axios.get('https://router.xieru.fun:12315/api/fs/list')\n            .then(response => {\n                this.videos = response.data.data.content;\n            })\n            .catch(error => {\n                console.error(error);\n            });\n    },\n    methods: {\n        formatDate(dateString) {\n            const date = new Date(dateString);\n            return date.toLocaleDateString();\n        },\n        formatSize(size) {\n            const fileSizeMB = (size / (1024 * 1024)).toFixed(2);\n            return `${fileSizeMB} MB`;\n        },\n    },\n};\n</script>\n\n<style scoped>\n.video-grid {\n    display: grid;\n    grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));\n    gap: 20px;\n}\n\n.video-item {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n}\n\n.video-thumbnail {\n    width: 100%;\n    height: auto;\n    object-fit: cover;\n    border-radius: 5px;\n}\n\n.video-info {\n    margin-top: 10px;\n    text-align: center;\n}\n\n.video-name {\n    font-size: 16px;\n    font-weight: bold;\n}\n\n.video-date,\n.video-size {\n    font-size: 14px;\n    margin-top: 5px;\n}\n</style>"],"mappings":"AAcA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE;IACZ,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNJ,KAAK,CAACK,GAAG,CAAC,4CAA4C,EACjDC,IAAI,CAACC,QAAO,IAAK;MACd,IAAI,CAACJ,MAAK,GAAII,QAAQ,CAACL,IAAI,CAACA,IAAI,CAACM,OAAO;IAC5C,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;MACZC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;IACxB,CAAC,CAAC;EACV,CAAC;EACDE,OAAO,EAAE;IACLC,UAAUA,CAACC,UAAU,EAAE;MACnB,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,UAAU,CAAC;MACjC,OAAOC,IAAI,CAACE,kBAAkB,CAAC,CAAC;IACpC,CAAC;IACDC,UAAUA,CAACC,IAAI,EAAE;MACb,MAAMC,UAAS,GAAI,CAACD,IAAG,IAAK,IAAG,GAAI,IAAI,CAAC,EAAEE,OAAO,CAAC,CAAC,CAAC;MACpD,OAAQ,GAAED,UAAU,KAAK;IAC7B;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}